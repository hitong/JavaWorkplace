<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane cacheHint="SCALE_AND_ROTATE" depthTest="ENABLE" prefHeight="690.0" prefWidth="985.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainViewController">
   <center>
      <ScrollPane prefHeight="604.0" prefWidth="862.0" BorderPane.alignment="CENTER">
         <content>
            <Pane fx:id="pane" />
         </content>
      </ScrollPane>
   </center>
   <right>
      <VBox cacheHint="QUALITY" depthTest="ENABLE" prefHeight="690.0" prefWidth="228.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#start" prefHeight="58.0" prefWidth="171.0" text="开始">
               <VBox.margin>
                  <Insets left="25.0" top="100.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#changePath" prefHeight="58.0" prefWidth="171.0" text="换一条路径">
               <VBox.margin>
                  <Insets left="25.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#reStart" prefHeight="58.0" prefWidth="171.0" text="恢复">
               <VBox.margin>
                  <Insets left="25.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#model" prefHeight="58.0" prefWidth="171.0" text="设置">
               <VBox.margin>
                  <Insets left="25.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#exit" prefHeight="58.0" prefWidth="171.0" text="退出">
               <VBox.margin>
                  <Insets left="25.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
   </right>
