package work;

import java.util.ArrayList;

import javafx.collections.ObservableList;

public class Search {
	public static ArrayList<Model> search(ObservableList<Model> codeData, String terget, int[] choise){
		ArrayList<Model> tmp = new ArrayList<>();
		
		for(int i = 0; i < codeData.size(); i++) {
			StringBuilder str = new StringBuilder("");
			for(int j = 0; j < choise.length; j++) {
				str.append(MySwitch.mySwitch(codeData.get(i) , choise[j]));
			}
			if(str.toString().contains(terget)) {
				tmp.add(codeData.get(i));
			}
		}
		return tmp;
	}
}
