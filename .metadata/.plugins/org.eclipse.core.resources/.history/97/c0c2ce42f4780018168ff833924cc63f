package model;

public class DownControl {
	private int len;
	private String fileId;
	private String from;
	private boolean endArr;
	private int numArr;
	
	public DownControl(){
		
	}
	
	public int getLen() {
		return len;
	}
	public void setLen(int len) {
		this.len = len;
	}
	public String getFileId() {
		return fileId;
	}
	public void setFileId(String fileId) {
		this.fileId = fileId;
	}
	public String getFrom() {
		return from;
	}
	public void setFrom(String from) {
		this.from = from;
	}
	public boolean isEndArr() {
		return endArr;
	}
	public void setEndArr(boolean endArr) {
		this.endArr = endArr;
	}
	public int getNumArr() {
		return numArr;
	}
	public void setNumArr(int numArr) {
		this.numArr = numArr;
	}
	
	public boolean isAllArr(){
		return numArr == len;
	}
}
