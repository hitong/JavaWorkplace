package localPattern;

public class MyChess {
	int oLocal;//外部坐标
	int iLocal;//内部坐标
	int board;//棋盘号
	public int getoLocal() {
		return oLocal;
	}
	public void setoLocal(int oLocal) {
		this.oLocal = oLocal;
	}
	public int getiLocal() {
		return iLocal;
	}
	public void setiLocal(int iLocal) {
		this.iLocal = iLocal;
	}
	public int getBoard() {
		return board;
	}
	public void setBoard(int board) {
		this.board = board;
	}
	
	
}
