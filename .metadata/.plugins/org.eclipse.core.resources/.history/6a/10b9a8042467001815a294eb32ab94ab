package application;

public class Depart {
	private int start;
	private int longth;
	private int status;
	
	public Depart(int start,int longth, int status){
		this.status = status;
		this.start = start;
		this.longth = longth;
	}
	
	public int getStart() {
		return start;
	}
	public int getLongth() {
		return longth;
	}
	public int getStatus() {
		return status;
	}
	
	public void setStatus(int status) {
		this.status = status;
	}
	
	public void setStart(int start) {
		this.start = start;
	}

	public void setLongth(int longth) {
		this.longth = longth;
	}

	public String toString(){
		return "Start:" + this.start + "    Longth: " + this.longth
				+ "    Status: " + this.status;
				
	}
}
